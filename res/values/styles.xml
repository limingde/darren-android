        <item name="android:textColor">@color/font4</item>
        <item name="android:textSize">@dimen/px_27</item>
    </style>

    <style name="MainTabText">
        <item name="android:textSize">12dp</item>
        <item name="android:textColor">@color/white</item>
    </style>

